{
  "name": "abovesky/think-permission",
  "description": "Permission handling for ThinkPHP",
  "type": "think-extend",
  "keywords": [
    "thinkphp",
    "permission",
    "permissions",
    "roles",
    "acl",
    "rbac",
    "security"
  ],
  "homepage": "https://github.com/abovesky/think-permission",
  "license": "MIT",
  "authors": [
    {
      "name": "abovesky",
      "email": "abovesky@qq.com"
    }
  ],
  "require": {
    "topthink/framework": "^6.0.0",
    "topthink/think-migration": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "think\\permission\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "think": {
      "config": {
        "permission": "src/config/permission.php"
      }
    }
  }
}
